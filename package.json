{
  "name": "prism-intelligence",
  "version": "1.0.0",
  "description": "AI-powered property management intelligence platform that transforms reports into actionable insights",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "node scripts/migrate.js",
    "queue:worker": "ts-node src/workers/reportProcessor.ts",
    "attachment-loop": "ts-node src/startAttachmentLoop.ts",
    "attachment-loop:dev": "nodemon --exec ts-node src/startAttachmentLoop.ts",
    "attachment-loop:build": "tsc && node dist/startAttachmentLoop.js"
  },
  "keywords": [
    "property-management",
    "ai",
    "analytics",
    "real-estate",
    "business-intelligence"
  ],
  "author": "Prism Intelligence Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@bull-board/api": "^5.17.0",
    "@bull-board/express": "^5.17.0",
    "@google/generative-ai": "^0.21.0",
    "@sendgrid/mail": "^8.1.3",
    "@supabase/supabase-js": "^2.43.4",
    "bull": "^4.12.2",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.4.1",
    "pdf-parse": "^1.1.1",
    "redis": "^4.6.14",
    "winston": "^3.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}